#Challenge1

total=0

while total<50:
  number=int(input("Enter a number"))
  total=total+number
  print("The total is",total)
  
#Challenge2

number=0

while number<5:
  number=int(input("Enter a number"))
  
  if number>5:
    print("The last number you entered was",number)

#Challenge3

number=int(input("Enter a number"))
total=number
letter="y"

while letter=="y":
  number2=int(input("Enter another number"))
  total=total+number2
  letter=input("Would you like to add another number")

print(total)

#Challenge4


control=1
count=0

while control==1:
  name=input("Who would you like to invite to the party?")
  print(name, "has now been invited.")
  count=count+1
  A=input("Would you like to ivite soembody else? Answer with y/n.")
  if A=="y":
    control=1
  else:
    control=2
      
print(count)

